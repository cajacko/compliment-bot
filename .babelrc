{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "models": "./src/server/models",
        "server": "./src/server",
        "constants": "./src/constants"
      },
      "extensions": [".js", ".jsx"]
      }
    ]
  ]
}
